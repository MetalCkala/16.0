        <ParameterColumn IdStr="Phys_Wsum" Name="Влажность&#xD;&#xA;суммарная&#xD;&#xA;W сумм., д.е." Group="Физические свойства" Visible="true" HiddenAsEmpty="false" Formula="" Format="0,0" ColumnRef="AD" SummaryFormula="" SummaryFormat="" />
        <ParameterColumn IdStr="Phys_Wzap" Name="Влажность&#xD;&#xA;заполнителя&#xD;&#xA;W зап., д.е." Group="Физические свойства" Visible="true" HiddenAsEmpty="false" Formula="" Format="0,0" ColumnRef="AE" SummaryFormula="" SummaryFormat="" />
        <ParameterColumn IdStr="Phys_Wl" Name="текучести  ,&#xD;&#xA;Wl, д.е." Group="Физические свойства|Пределы пластичности" Visible="true" HiddenAsEmpty="false" Formula="" Format="" ColumnRef="R" SummaryFormula="" SummaryFormat="0" />
        <ParameterColumn IdStr="Phys_Wp" Name="раската  ,&#xD;&#xA;Wp, д.е.&#xD;&#xA;" Group="Физические свойства|Пределы пластичности" Visible="true" HiddenAsEmpty="false" Formula="" Format="" ColumnRef="S" SummaryFormula="" SummaryFormat="0" />
        <ParameterColumn IdStr="Phys_Ip" Name="Число пла-&#xD;&#xA;стичности&#xD;&#xA;Ip, д.е.&#xD;&#xA;" Group="Физические свойства" Visible="true" HiddenAsEmpty="false" Formula="=IF(V($Phys_Wl)=0,0,IF( V($Phys_Wl)=V($Phys_Wp),0.0001,$Phys_Wl-$Phys_Wp))" Format="0;0;;" ColumnRef="T" SummaryFormula="=V($Phys_Wl) - V($Phys_Wp)" SummaryFormat="0;0;;" />
        <ParameterColumn IdStr="Phys_Il" Name="Показатель&#xD;&#xA;текучести,&#xD;&#xA;Il, д.е. " Group="Физические свойства" Visible="true" HiddenAsEmpty="false" Formula="=IF(V($Phys_W)&gt;0, IF(V($Phys_Ip)&gt;0,IF(V($Phys_Wp)&gt;0,(V($Phys_W)-V($Phys_Wp))/V($Phys_Ip),0),0), IF(V($Phys_Wzap)&gt;0,IF(V($Phys_Ip)&gt;0,IF(V($Phys_Wp)&gt;0,(V($Phys_Wzap)-V($Phys_Wp))/V($Phys_Ip),0),0),0))" Format="0,00;&quot;&lt;0&quot;;;" ColumnRef="U" SummaryFormula="=IF(V($Phys_Ip)&gt;0, (V($Phys_W)-V($Phys_Wp))/V($Phys_Ip), &quot;&quot;)" SummaryFormat="" />
        <ParameterColumn IdStr="Phys_Density" Name="влажного&#xD;&#xA;ρ" Group="Физические свойства|Плотность грунта, г/см^3" Visible="true" HiddenAsEmpty="false" Formula="" Format="0,00" ColumnRef="V" SummaryFormula="" SummaryFormat="" />
        <ParameterColumn IdStr="Phys_Density_d" Name="скелета&#xD;&#xA;ρd" Group="Физические свойства|Плотность грунта, г/см^3" Visible="true" HiddenAsEmpty="false" Formula="=IF(V($Phys_W)&gt;0,V($Phys_Density)/(1+0.01*V($Phys_W)),IF(V($Phys_Wzap)&gt;0,V($Phys_Density)/(1+0.01*V($Phys_Wzap)),0))" Format="0,00;&quot;&lt; 0&quot;;;" ColumnRef="W" SummaryFormula="=IF(V($Phys_Density)&gt;0, V($Phys_Density)/(1+V($Phys_W)*0.01), &quot;&quot;)" SummaryFormat="" />
        <ParameterColumn IdStr="Phys_Density_s" Name="частиц&#xD;&#xA;ρs" Group="Физические свойства|Плотность грунта, г/см^3" Visible="true" HiddenAsEmpty="false" Formula="" Format="0,00" ColumnRef="X" SummaryFormula="" SummaryFormat="" />
        <ParameterColumn IdStr="Phys_e" Name="Коэфф.&#xD;&#xA;порис-&#xD;&#xA;тости, e&#xD;&#xA;" Group="Физические свойства" Visible="true" HiddenAsEmpty="false" Formula="=IF(V($Phys_Density)&gt;0,(IF(V($Phys_Density_d)&gt;0,(V($Phys_Density_s)-V($Phys_Density_d))/V($Phys_Density_d),0)),0)" Format="0,00;-0,00;;" ColumnRef="Y" SummaryFormula="=IF(V($Phys_Density)&gt;0, (V($Phys_Density_s)-V($Phys_Density_d))/V($Phys_Density_d), &quot;&quot;)" SummaryFormat="" />
        <ParameterColumn IdStr="Phys_n" Name="Порис-&#xD;&#xA;тость,&#xD;&#xA;n, %&#xD;&#xA;" Group="Физические свойства" Visible="true" HiddenAsEmpty="false" Formula="=V($Phys_e)/(1+V($Phys_e))*100" Format="0,0;;;" ColumnRef="Z" SummaryFormula="" SummaryFormat="" />
        <ParameterColumn IdStr="Phys_Sr" Name="Коэфф.&#xD;&#xA;водонасыщ.&#xD;&#xA;Sr, д.е." Group="Физические свойства" Visible="true" HiddenAsEmpty="false" Formula="=IF(V($Phys_W)&gt;0,IF(V($Phys_e)&gt;0,(IF(V($Phys_W)*V($Phys_Density_s)&gt;V($Phys_e)*100,1,(V($Phys_W)*V($Phys_Density_s))/V($Phys_e)/100)),0),IF(V($Phys_Wzap)&gt;0,IF(V($Phys_e)&gt;0,(IF(V($Phys_Wzap)*V($Phys_Density_s)&gt;V($Phys_e)*100,1,(V($Phys_Wzap)*V($Phys_Density_s))/V($Phys_e)/100)),0),0))" Format="0,00;-0,00;;" ColumnRef="AA" SummaryFormula="=IF(V($Phys_Density)&gt;0, V($Phys_Density_s)*V($Phys_W)*0.01/V($Phys_e), &quot;&quot;)" SummaryFormat="" />
        <ParameterColumn IdStr="Mech_Fi" Name="Угол внутр.&#xD;&#xA;трения,&#xD;&#xA;φ, град." Group="Физические свойства" Visible="true" HiddenAsEmpty="false" Formula="" Format="0" ColumnRef="AP" SummaryFormula="" SummaryFormat="" />
        <ParameterColumn IdStr="Mech_C" Name="Удельное&#xD;&#xA;сцепление&#xD;&#xA;C, МПа" Group="Физические свойства" Visible="true" HiddenAsEmpty="false" Formula="" Format="0,00" ColumnRef="AQ" SummaryFormula="" SummaryFormat="" />
        <ParameterColumn IdStr="Mech_Ek" Name="Ek" Group="Физические свойства|Модуль дефор-&#xD;&#xA;мации, МПа&#xD;&#xA;" Visible="true" HiddenAsEmpty="false" Formula="" Format="0,00" ColumnRef="AR" SummaryFormula="" SummaryFormat="" />
        <ParameterColumn IdStr="Mech_Ek_mk" Name="с учетом m_k" Group="Физические свойства|Модуль дефор-&#xD;&#xA;мации, МПа&#xD;&#xA;" Visible="true" HiddenAsEmpty="false" Formula="" Format="0,00" ColumnRef="AS" SummaryFormula="" SummaryFormat="" />
        <ParameterColumn IdStr="Mech_EkW" Name="EkW" Group="Физические свойства|Модуль дефор-&#xD;&#xA;мации замоч., МПа" Visible="true" HiddenAsEmpty="false" Formula="" Format="0,00" ColumnRef="" SummaryFormula="" SummaryFormat="" />
        <ParameterColumn IdStr="Mech_EkW_mk" Name="с учетом m_k" Group="Физические свойства|Модуль дефор-&#xD;&#xA;мации замоч., МПа" Visible="true" HiddenAsEmpty="false" Formula="" Format="0,00" ColumnRef="" SummaryFormula="" SummaryFormat="" />
        <ParameterColumn IdStr="Phys_Wopt" Name="Оптимальная&#xD;&#xA;влажность,&#xD;&#xA;Wopt, д.е." Group="Физические свойства" Visible="true" HiddenAsEmpty="false" Formula="" Format="0,0" ColumnRef="AL" SummaryFormula="" SummaryFormat="" />
        <ParameterColumn IdStr="Phys_Max_Density_d" Name="Макс. плотн.&#xD;&#xA;скелета,&#xD;&#xA;г/см^3" Group="Физические свойства" Visible="true" HiddenAsEmpty="false" Formula="" Format="0,0" ColumnRef="AM" SummaryFormula="" SummaryFormat="" />
        <ParameterColumn IdStr="Org_Ir" Name="Содержание&#xD;&#xA;орг. вещества&#xD;&#xA;Ir, д.е." Group="Органические свойства" Visible="true" HiddenAsEmpty="false" Formula="" Format="0,0" ColumnRef="AB" SummaryFormula="" SummaryFormat="" />
        <ParameterColumn IdStr="Org_Ddp" Name="Степень &#xD;&#xA;разл-я торфа&#xD;&#xA;Ddp,%" Group="Органические свойства" Visible="true" HiddenAsEmpty="false" Formula="" Format="0" ColumnRef="AC" SummaryFormula="" SummaryFormat="" />
        <ParameterColumn IdStr="Org_Das" Name="Степень золь-&#xD;&#xA;ности торфа&#xD;&#xA;Das, д.е. " Group="Органические свойства" Visible="true" HiddenAsEmpty="false" Formula="" Format="" ColumnRef="" SummaryFormula="" SummaryFormat="" />
        <ParameterColumn IdStr="Org_Humus" Name="Процент&#xD;&#xA;гумуса, %" Group="Органические свойства" Visible="true" HiddenAsEmpty="false" Formula="" Format="" ColumnRef="" SummaryFormula="" SummaryFormat="" />
