        <ParameterColumn IdStr="GMC_Aw" Name="Угол откоса&#xD;&#xA;замоченного&#xD;&#xA;грунта, град." Group="Гранметрические свойства" Visible="true" HiddenAsEmpty="false" Formula="" Format="0" ColumnRef="AG" SummaryFormula="" SummaryFormat="" />
        <ParameterColumn IdStr="GMC_Density_FilterCoeff_Soft" Name="ρ к.ф. рых.,&#xD;&#xA;г / см^3" Group="Гранметрические свойства" Visible="true" HiddenAsEmpty="false" Formula="" Format="0,00" ColumnRef="AJ" SummaryFormula="" SummaryFormat="" />
        <ParameterColumn IdStr="GMC_Density_FilterCoeff_Hard" Name="ρ к.ф. пл.,&#xD;&#xA;г / см^3" Group="Гранметрические свойства" Visible="true" HiddenAsEmpty="false" Formula="" Format="0,00" ColumnRef="AK" SummaryFormula="" SummaryFormat="" />
        <ParameterColumn IdStr="GMC_FilterCoeff_Soft" Name="к.ф. рых.,&#xD;&#xA;г / см^3" Group="Гранметрические свойства" Visible="true" HiddenAsEmpty="false" Formula="" Format="0,00000" ColumnRef="AH" SummaryFormula="" SummaryFormat="" />
        <ParameterColumn IdStr="GMC_FilterCoeff_Hard" Name="к.ф. пл.,&#xD;&#xA;г / см^3" Group="Гранметрические свойства" Visible="true" HiddenAsEmpty="false" Formula="" Format="0,00000" ColumnRef="AI" SummaryFormula="" SummaryFormat="" />
