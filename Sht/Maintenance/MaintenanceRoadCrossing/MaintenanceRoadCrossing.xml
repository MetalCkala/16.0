  <Table id="Maintenance" name="Пересечения автомобильных дорог">
    <Subtable name="Заголовок" id="Head">
      <Row index="0" height="7" options="2">
        <Cell index="0" colspan="13" style="style_0">$(getvar, FileName)</Cell>
      </Row>
      <Row index="1" height="7">
        <Cell index="0" rowspan="3" style="style_0">Адрес участка пк+</Cell>
        <Cell index="1" rowspan="3" style="style_0">Номер направления движения: 1-прямое, 2-обратное</Cell>
        <Cell index="2" colspan="4" style="style_0">Левоповоротные съезды (въезды)</Cell>
        <Cell index="6" colspan="4" style="style_0">Правоповоротные съезды (въезды)</Cell>
        <Cell index="10" colspan="3" style="style_0">Протяженность съездов (въездов), м</Cell>
      </Row>
      <Row index="2" height="7">
        <Cell index="2" rowspan="2" style="style_0">Код типа размещения</Cell>
        <Cell index="3" rowspan="2" style="style_0">Коды дефектов</Cell>
        <Cell index="4" colspan="2" style="style_0">Направление движения</Cell>
        <Cell index="6" rowspan="2" style="style_0">Код типа размещения</Cell>
        <Cell index="7" rowspan="2" style="style_0">Коды дефектов</Cell>
        <Cell index="8" colspan="2" style="style_0">Направление движения</Cell>
        <Cell index="10" colspan="2" style="style_0">Основных</Cell>
        <Cell index="12" rowspan="2" style="style_0">Дополнительных</Cell>
      </Row>
      <Row index="3" height="7">
        <Cell index="4" style="style_0">На дорогу (имя)</Cell>
        <Cell index="5" style="style_0">К объекту (имя)</Cell>
        <Cell index="8" style="style_0">На дорогу (имя)</Cell>
        <Cell index="9" style="style_0">К объекту (имя)</Cell>
        <Cell index="10" style="style_0">Левоповоротных</Cell>
        <Cell index="11" style="style_0">Правоповоротных</Cell>
      </Row>
    </Subtable>
    <Subtable name="Параметры" id="MaintenanceData">
      <Row index="0" height="5" options="2">
        <Cell index="0" style="style_0">$(getvar, Piket)</Cell>
        <Cell index="1" style="style_0">$(getvar, Direction)</Cell>
        <Cell index="2" style="style_0">$(getvar, LocationCodeLeft)</Cell>
        <Cell index="3" style="style_0">$(getvar, DefectsLeft)</Cell>
        <Cell index="4" style="style_0">$(getvar, MoveDirectionLeft)</Cell>
        <Cell index="5" style="style_0">$(getvar, MoveDirectionLeft2)</Cell>
        <Cell index="6" style="style_0">$(getvar, LocationCodeRight)</Cell>
        <Cell index="7" style="style_0">$(getvar, DefectsRight)</Cell>
        <Cell index="8" style="style_0">$(getvar, MoveDirectionRight)</Cell>
        <Cell index="9" style="style_0">$(getvar, MoveDirectionRight2)</Cell>
        <Cell index="10" style="style_0">$(getvar, RoadEntryLengthLeft)</Cell>
        <Cell index="11" style="style_0">$(getvar, RoadEntryLengthRight)</Cell>
        <Cell index="12" style="style_0">$(getvar, RoadEntryLengthAdditional)</Cell>
      </Row>
    </Subtable>
