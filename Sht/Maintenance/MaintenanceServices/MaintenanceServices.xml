        <Cell index="9" style="style_0">Торговый павильон</Cell>
        <Cell index="10" style="style_0">Прочее</Cell>
      </Row>
    </Subtable>
    <Subtable name="Параметры" id="MaintenanceData">
      <Row index="0" height="5" options="2">
        <Cell index="0" style="style_0">$(getvar, Piket)</Cell>
        <Cell index="1" style="style_0">$(getvar, ObjectCode)</Cell>
        <Cell index="2" style="style_0">$(getvar, Side)</Cell>
        <Cell index="3" style="style_0">$(getvar, Shop)</Cell>
        <Cell index="4" style="style_0">$(getvar, Shop2)</Cell>
        <Cell index="5" style="style_0">$(getvar, Cafe)</Cell>
        <Cell index="6" style="style_0">$(getvar, Bar)</Cell>
